<trackerinfo
	type="tg"
	shortName="tg"
	longName="TorrentGigs"
	siteName="">

	<settings>
		<description text="Paste the RSS feed for your passkey."/>
		<passkey/>
	</settings>

	<servers>
		<server
			network="TorrentGigs"
			serverNames="irc.btgigs.info"
			channelNames="#pre"
			announcerNames="Info"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!-- [REQ]  Twin Peaks: Ogniu Krocz Ze MnÂ± / Twin Peaks Fire Walk With Me 1992 PL 720p BRRip XviD AC3-inTGrity - http://btgigs.info/details.php?id=356855-->
				<!---->
				<!--Nagi Instynkt Przetrwania / Naked And Afraid S02E06 Meltdown In Bolivia PL 720p HDTV X264 AC3-TVM4iN - http://btgigs.info/details.php?id=356858-->
				<regex value="^(.*)\s\-\s*https?\:\/\/([^\/]+\/).*[&amp;\?]id=(\d+)"/>
				<vars>
					<var name="torrentName"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="torrentUrl">
				<string value="http://"/>
				<var name="$baseUrl"/>
				<string value="download_rss.php/"/>
				<var name="$torrentId"/>
				<string value="/"/>
				<varenc name="passkey"/>
				<string value="/0/"/>
				<varenc name="torrentName"/>
				<string value=".torrent"/>
				
			</var>
		</linematched>
		<ignore>
		</ignore>
	</parseinfo>
</trackerinfo>
