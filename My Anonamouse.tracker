<trackerinfo
	type="mam"
	shortName="MAM"
	longName="My Anonamouse"
	siteName="www.myanonamouse.net">

	<settings>
		<cookie_description/>
		<description text="&lt;br&gt;&lt;strong&gt;Required Keys:&lt;/strong&gt; mam_id (Changes monthly)."/>
		<cookie/>
	</settings>

	<servers>
		<server
			network="ANONAMOUSE"
			serverNames="irc.myanonamouse.net"
			channelNames="#announce"
			announcerNames="MouseBot"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!--New Torrent :  A Madman Dreams of Turing Machines [REQUESTED] Category: ( Ebooks - General Non-Fiction ) Size: ( 1.66 MB ) Link: ( https://www.myanonamouse.net/t/199645 )-->
				<regex value="New Torrent :(.*) Category:\s*\((.*)\) Size: \((.*)\) Link: \(\s*https?\:\/\/([^\/]+\/).*?(\d+)"/>
				<vars>
					<var name="torrentName"/>
					<var name="category"/>
					<var name="torrentSize"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="torrentUrl">
				<string value="https://"/>
				<var name="$baseUrl"/>
				<string value="download.php/"/>
				<var name="$torrentId"/>
				<string value="/"/>
				<varenc name="torrentName"/>
				<string value=".torrent"/>
			</var>
			<http name="cookie">
				<var name="cookie"/>
			</http>
		</linematched>
		<ignore>
		</ignore>
	</parseinfo>
</trackerinfo>
