<trackerinfo
	type="teh"
	shortName="TC"
	longName="TehConnection.eu"
	siteName="tehconnection.eu">

	<settings>
		<gazelle_description/>
		<gazelle_authkey/>
		<gazelle_torrent_pass/>
	</settings>

	<servers>
		<server
			network="P2P"
			serverNames="irc.tehconnection.eu,irc.p2p-net.eu"
			channelNames="#TehConnection"
			announcerNames="justine"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!--New Torrent: The War On Kids (2009) [DivX/Standard Def/AVI/DVD] :: Uploader: bangbaby :: https://tehconnection.eu/torrents.php?id=10061&torrentid=18268-->
				<!--New Torrent: Les Quatre cents coups (1959) [x264/720p/Matroska/Blu-Ray/Scene] :: Special Edition: Criterion Collection #5 (1959) :: Uploader: ezekirby :: https://tehconnection.eu/torrents.php?id=292&torrentid=18320-->
				<regex value="^New Torrent: (.*) \[(.*)\] :: (.*) :: Uploader: (.*) :: https?\:\/\/([^\/]+\/).*[&amp;\?]torrentid=(\d+)"/>
				<vars>
					<var name="torrentName"/>
					<var name="category"/>
					<var name="torrentSize"/>
					<var name="uploader"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="scene">
				<string value="false"/>
			</var>

			<varreplace name="tags" srcvar="category" regex="[\/]" replace=", "/>

			<extract srcvar="torrentName" optional="true">
				<regex value="(?:^|[^\d])((?:18|19|20)\d\d)(?:$|[^\d])"/>
				<vars>
					<var name="year"/>
				</vars>
			</extract>

			<extracttags srcvar="tags" split=",">
				<setvarif varName="scene" value="Scene" newValue="true"/>

				<setvarif varName="resolution" regex="^(?:SD|Standard?Def.*|480i|480p|576p|720p|810p|1080p|1080i|PD|Portable Device)$"/>
				<setvarif varName="source" regex="^(?:R5|DVDScr|BRRip|CAM|TS|TELESYNC|TC|TELECINE|DSR|PDTV|HDTV|DVDRip|BDRip|DVDR|BluRay|Blu\-Ray|WEBRip|WEB\-DL|WEB|TVRip|HDDVD|HD\-DVD)$"/>
				<setvarif varName="encoder" regex="^(?:XviD|DivX|x264|h\.264|h264|mpeg2|VC\-1|VC1|WMV)$"/>

				<!--Ignored-->
				<regex value=""/>
			</extracttags>

			<var name="torrentUrl">
				<string value="http://"/>
				<var name="$baseUrl"/>
				<string value="torrents.php?action=download&amp;id="/>
				<var name="$torrentId"/>
				<string value="&amp;authkey="/>
				<var name="authkey"/>
				<string value="&amp;torrent_pass="/>
				<var name="torrent_pass"/>
			</var>
		</linematched>
		<ignore>
			<regex value="^New Torrent:" expected="false"/>
		</ignore>
	</parseinfo>
</trackerinfo>
