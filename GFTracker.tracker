<trackerinfo
	type="gft"
	shortName="GFT"
	longName="GFTracker"
	siteName="www.thegft.org">

	<settings>
		<description text="Go to http://www.thegft.org/getrss.php to get the RSS feed link. Paste it (Ctrl+V) into the text box below to automatically extract passkey."/>
		<passkey tooltiptext="The passkey in your GFTracker RSS feed link."/>
	</settings>

	<servers>
		<server
			network="Station51"
			serverNames="irc.thegft.org,irc.station51.net"
			channelNames="#gftracker-spam"
			announcerNames="Spannage"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<!--NEW :: JaydenJaymes.12.12.01.Jayden.Jaymes.And.Sandy.Pretty.In.Pink.XXX.WMV-YAPG :: XXX/0DAY :: http://www.thegft.org/details.php?id=889191 :: 616.77 MB :: 28 seconds after pre.-->
				<regex value="^NEW\s*::(.*)::(.*)::\s*https?\:\/\/([^\/]+\/).*[&amp;\?]id=(\d+)\s*::(.*)::(.*)"/>
				<vars>
					<var name="torrentName"/>
					<var name="category"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
					<var name="torrentSize"/>
					<var name="preTime"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="torrentUrl">
				<string value="http://"/>
				<var name="$baseUrl"/>
				<string value="download.php?torrent="/>
				<var name="$torrentId"/>
				<string value="&amp;passkey="/>
				<varenc name="passkey"/>
			</var>
		</linematched>
		<ignore>
		</ignore>
	</parseinfo>
</trackerinfo>
