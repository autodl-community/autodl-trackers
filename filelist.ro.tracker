<trackerinfo
	type="flr"
	shortName="FLR"
	longName="filelist.ro"
	siteName="filelist.ro">

	<settings>
		<description text="Go to http://filelist.ro/getrss.php to get the RSS feed link. Paste it (Ctrl+V) into the text box below to automatically extract passkey."/>
		<passkey/>
	</settings>

	<servers>
		<server
			network="P2P-NET"
			serverNames="irc.p2p-network.net,irc.p2p-irc.net"
			channelNames="#filelist"
			announcerNames="Announce"
			/>
	</servers>

	<parseinfo>
		<linepatterns>
			<extract>
				<regex value="^New Torrent:\s*(.*)\s*--\s*(.*)\s*--\s*https?\:\/\/([^\/]+\/).*[&amp;\?]id=(\d+).*by\s*(.*)"/>
				<vars>
					<var name="torrentName"/>
					<var name="$releaseTags"/>
					<var name="$baseUrl"/>
					<var name="$torrentId"/>
					<var name="uploader"/>
				</vars>
			</extract>
		</linepatterns>
		<linematched>
			<var name="freeleech">
				<string value="false"/>
			</var>

			<setregex srcvar="$releaseTags" regex="[FreeLeech!]" varName="freeleech" newValue="true"/>
			<varreplace name="$releaseTags" srcvar="$releaseTags" regex="[FreeLeech!]\s*" replace=""/>

			<extract srcvar="$releaseTags">
				<regex value="\[(.*?)\]\s*\[(.*)\]"/>
				<vars>
					<var name="category"/>
					<var name="torrentSize"/>
				</vars>
			</extract>

			<var name="torrentUrl">
				<string value="http://"/>
				<var name="$baseUrl"/>
				<string value="download.php?id="/>
				<var name="$torrentId"/>
				<string value="&amp;file="/>
				<varenc name="torrentName"/>
				<string value=".torrent&amp;passkey="/>
				<var name="passkey"/>
			</var>
		</linematched>
		<ignore>
			<regex value="^New!\s*(.*)-" expected="false"/>
		</ignore>
	</parseinfo>
</trackerinfo>
